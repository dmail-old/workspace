		{
			"type": "node",
			"protocol": "inspector",
			"request": "launch",
			"name": "Launch with babel-node",
			"preLaunchTask": null,
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
			"runtimeArgs": ["--nolazy"],
			"stopOnEntry": false,
			"env": {
				"NODE_ENV": "development"
				// "BABEL_CACHE_PATH": "C:\\Users\\Damien\\Dev\\cache\\babel.json"
			},
			// "console": "internalConsole",
			// "outFiles": [],
			"sourceMaps": true,
			"program": "${file}",
			"skipFiles": ["packages/signal/src/**"]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attacher au port",
			"address": "localhost",
			"port": 5858
		}
	]
