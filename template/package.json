{
	"name": "template",
	"version": "0.0.1",
	"licence": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dmail/template"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "index.js",
	"dependencies": {},
	"devDependencies": {
		"@dmail/shared-config": "*",
		"@dmail/command": "*",
		"@dmail/prettiest": "*",
		"@dmail/test": "*",
		"babel-cli": "^7.0.0-beta.0",
		"babel-core": "^7.0.0-beta.0",
		"babel-eslint": "^8.0.0",
		"babel-preset-env": "^2.0.0-beta.0",
		"eslint": "^3.19.0",
		"eslint-plugin-import": "^2.7.0",
		"prettier": "^1.7.0",
		"rimraf": "^2.6.2"
	},
	"scripts": {
		"compile": "babel . --out-dir dist --ignore=node_modules,dist --source-maps",
		"watch": "babel . --watch --out-dir dist --ignore=node_modules,dist --source-maps",
		"build": "npm run compile",
		"clean": "rimraf dist",
		"dist": "npm run clean && npm run compile",
		"prepublishOnly": "npm run dist",
		"eslint": "eslint .",
		"prettiest": "prettiest",
		"test": "test",
		"pretest": "npm run eslint && npm run prettiest && npm run compile"
	}
}
